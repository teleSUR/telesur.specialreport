<?xml version="1.0"?>
<object name="telesur.specialreport" meta_type="Dexterity FTI" i18n:domain="plone"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">Flash special</property>
 <property name="description"
    i18n:translate="">Content type to upload flash files.</property>
 <property name="icon_expr">string:${portal_url}/document_icon.png</property>
 <property name="factory">telesur.specialreport</property>
 <property
    name="add_view_expr">string:${folder_url}/++add++telesur.specialreport</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <property name="default_view">telesurspecialreportview</property>
 <property name="view_methods">
  <element value="telesurspecialreportview"/>
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">cmf.AddPortalContent</property>
 <property name="klass">plone.dexterity.content.Container</property>
 <property name="behaviors">
  <element value="plone.app.dexterity.behaviors.metadata.IDublinCore"/>
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element value="collective.behavior.localdiazo.behavior.ILocalDiazo"/>
  <element value="collective.behavior.localregistry.behavior.ILocalRegistry"/>
 </property>
 <property name="schema"></property>
 <property
    name="model_source">&lt;model xmlns="http://namespaces.plone.org/supermodel/schema"&gt;
    &lt;schema&gt;
      &lt;field name="file" type="plone.namedfile.field.NamedFile"&gt;
        &lt;description /&gt;
        &lt;title&gt;file&lt;/title&gt;
      &lt;/field&gt;
      &lt;field name="image" type="plone.namedfile.field.NamedBlobImage"&gt;
        &lt;description /&gt;
        &lt;title&gt;Image&lt;/title&gt;
      &lt;/field&gt;
      &lt;field name="width" type="zope.schema.Int"&gt;
        &lt;description /&gt;
        &lt;default&gt;640&lt;/default&gt;
        &lt;title&gt;Width&lt;/title&gt;
      &lt;/field&gt;
      &lt;field name="height" type="zope.schema.Int"&gt;
        &lt;description /&gt;
        &lt;default&gt;480&lt;/default&gt;
        &lt;title&gt;Height&lt;/title&gt;
      &lt;/field&gt;
    &lt;/schema&gt;
  &lt;/model&gt;</property>
 <property name="model_file"></property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
